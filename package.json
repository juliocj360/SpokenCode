{
  "name": "spoken-code",
  "main": "./lib/spoken-code",
  "version": "1.7.0",
  "description": "An IDE plugin for developers who would like their JavaScript code read to them, out loud.",
  "keywords": [
    "Atom"
  ],
  "scripts": {
    "tdd": "mocha -w -R nyan",
    "test:nyc": "nyc tap ./test/*.js",
    "test": "nyc --reporter=lcov --reporter=text-lcov mocha",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": "https://github.com/juliocj360/spoken-code",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "esprima": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "codecov": "^3.0.0",
    "mocha": "^5.0.0",
    "nyc": "^13.2.0"
  }
}
