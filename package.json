{
  "name": "spoken-code",
  "main": "./lib/spoken-code",
  "version": "1.4.0",
  "description": "An IDE plugin for developers who would like their JavaScript code read to them, out loud.",
  "keywords": [
    "Atom"
  ],
  "configSchema": {
    "userName": {
      "type": "string",
      "description": "Please sign up for IBM's Bluemix Text-to-Speech API [here](https://console.ng.bluemix.net/catalog/services/text-to-speech/) and enter the details below.",
      "default": "",
      "order": 1
    },
    "password": {
      "type": "string",
      "default": "",
      "order": 2
    }
  },
  "scripts": {
    "tdd": "mocha -w -R nyan",
    "test:nyc": "nyc tap ./test/*.js",
    "test": "nyc --reporter=lcov --reporter=text-lcov mocha"
  },
  "repository": "https://github.com/juliocj360/spoken-code",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "esprima": "^3.1.3",
    "watson-developer-cloud": "^2.18.0",
    "watson-speech": "^0.32.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2"
  }
}
